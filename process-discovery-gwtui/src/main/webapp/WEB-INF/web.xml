<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>dummyHttpServlet</servlet-name>
    <servlet-class>com.worpdress.salaboy.smartprocessdiscovery.server.DummyHttpServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>dummyHttpServlet</servlet-name>
    <url-pattern>/dummyHttpServlet/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>dummyHttpServlet</servlet-name>
    <url-pattern>/SmartProcessDiscovery/dummyHttpServlet</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>SmartProcessDiscovery.html</welcome-file>
  </welcome-file-list>

</web-app>
